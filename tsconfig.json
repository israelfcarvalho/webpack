{
    "compilerOptions": {

        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "ESNext",
        "target": "es6",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "isolatedModules": true,
        "moduleResolution": "node",
    },
    "exclude": ["node_modules", "dist"]
}